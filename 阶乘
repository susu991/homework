#include<iostream>
using namespace std;
int factorial(int n)
{
    if(n==1) {return 1;}
    return factorial(n-1)*n;
}
int main()
{
    cout<<factorial(3)<<endl;
}//递归



#include <iostream>
using namespace std;
int factorial(int n)
{
	int num;
	int result=1;
	if(n==1) return 1;
	for (num=2;num<=n;num++)
	{
		result *= num;
	}
	return result;
}
int main()
{
	cout<<factorial(3)<<endl;
}//递推
